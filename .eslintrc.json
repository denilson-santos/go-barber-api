{
  "extends": ["@caumeira/eslint-config/backend"],
  "settings": {
    "jest": {
      "version": "lastest"
    }
  },
  "rules": {
    "class-methods-use-this": "off",
    "camelcase": "off",
    "@typescript-eslint/naming-convention": [
      "error",
      {
        "selector": "interface",
        "format": ["PascalCase"],
        "custom": {
          "regex": "^I[A-Z]",
          "match": true
        }
      }
    ]
  }
  // ,
  // "overrides": [
  //   {
